{
  "name": "My Bookmarks",
  "version": "1.0",
  "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "permissions": [
    "bookmarks",
    "http://*/*", 
    "https://*/*"
  ],
  
  "background": {
  	"scripts": ["back.js"]
   },
  "content_security_policy": "script-src 'self' http://localhost:8080/EasyMark/extensionUserCheck; object-src 'self'",
  
  "browser_action": {
      "default_title": "My Bookmarks.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
