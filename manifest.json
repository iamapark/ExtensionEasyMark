{
  "name": "EasyMark",
  "version": "1.0",
  "description": "easymark.pe.kr에 손쉽게 북마크할 수 있는 확장프로그램입니다.",
  "permissions": [
    "bookmarks",
    "http://*/*", 
    "https://*/*",
    "tabs"
  ],
  
  "background": {
  	"scripts": ["back.js"]
   },
  "content_security_policy": "script-src 'self' http://localhost:8080/EasyMark/; object-src 'self'",
  
  "browser_action": {
      "default_title": "EasyMark",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
